{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "description": "A representation of an event",
  "title": "TestScript",
  "type": "object",
  "properties": {
    "jarAlias": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_\\-]+\\.jar$"
    },
    "jarUrl": {
      "type": "string",
      "format": "uri"
    }
  },
  "required": ["jarAlias", "jarUrl"]
}